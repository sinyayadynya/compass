            {# Get menu items #}
            {% set items = simplify_menu('main') %}
            {# Iterate menu tree #}
            {% for menu_item in items.menu_tree %}
            {% set active = (menu_item.active) ? 'is-active ' : ' ' %}
            <a href="{{ menu_item.url }}" class="{{ active }} text-gray-950 dark:text-white">
                {{ menu_item.text }}
            </a>
            {% endfor %}
            <button
                class="inline-flex items-center gap-x-2 focus:not-data-focus:outline-none"
                id="headlessui-menu-button-«r2»"
                type="button" aria-haspopup="menu" aria-expanded="false"
                data-headlessui-state="">
                Account
                <svg viewBox="0 0 8 4" fill="none"
                    class="stroke-gray-950 dark:stroke-white h-1 shrink-0">
                    <path fill="none" d="M1 0.5L4 3.5L7 0.5" stroke-linecap="round" stroke-linejoin="round"></path>
                </svg>
            </button>
        </div>
    </nav>
